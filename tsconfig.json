{
  "compilerOptions": {
    "checkJs": true,
    "noEmit": true,
    "lib": ["DOM", "ES6", "ES2019"],
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "strict": true,
    "downlevelIteration": true,
    "module": "ES6",
    "moduleResolution": "node",
    "target": "ES6",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["./components/*"],
      "@components": ["./components/"],
      "@lib/*": ["./lib/*"],
      "@lib": ["./lib/"]
    }
  },
  "include": [".aha-cache/types", "src"]
}
