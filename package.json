{
  "name": "aha-develop/github",
  "description": "GitHub integration extension",
  "version": "0.0.0",
  "author": "Aha!",
  "respository": {
    "type": "git",
    "url": "https://github.com/aha-develop/github"
  },
  "dependencies": {
  },
  "license": "MIT",
  "ahaExtension": {
    "contributes": {
      "customFields": {
        "url": {
          "title": "GitHub URL",
          "entryPoint": "src/component.js",
          "recordTypes": ["Feature", "Epic", "Requirement"],
          "type": "UrlField"
        },
        "links": {
          "title": "GitHub links",
          "entryPoint": "src/links.js",
          "recordTypes": ["Feature", "Epic", "Requirement"],
          "type": "UrlField"
        }
      },
      "views": {
        "auth": {
          "title": "Authenticate to GitHub",
          "entryPoint": "src/auth.js",
          "host": "modal"
        }
      },
      "commands": {
        "createBranch": {
          "title": "GitHub: Create a branch",
          "entryPoint": "src/createBranch.js"
        },
        "sync": {
          "title": "GitHub: Sync pull requests",
          "entryPoint": "src/sync.js"
        }
      },
      "endpoints": {
        "fetch_pull_details": {
          "title": "Fetch pull details",
          "entryPoint": "src/endpoint.js"
        },
        "webhook": {
          "title": "Webhook from Github",
          "entryPoint": "src/webhook.js",
          "public": true
        }
      }

    }
  }
}
